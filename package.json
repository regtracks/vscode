{
    "name": "regtracks",
    "displayName": "RegTracks syntax highlighting",
    "description": "Syntax highlighting for RegTracks",
    "version": "0.1.0",
    "publisher": "jamesthistlewood",
    "repository": {
        "url": "https://github.com/regtracks/vscode/"
    },
    "engines": {
        "vscode": "^1.39.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "regtracks",
            "aliases": ["RegTracks", "regtracks"],
            "extensions": [".track"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "regtracks",
            "scopeName": "source.regtracks",
            "path": "./syntaxes/regtracks.tmLanguage.json"
        }]
    }
}